{
	"name": "ds_github_raw_file",
	"properties": {
		"linkedServiceName": {
			"referenceName": "ls_git_raw_http",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"owner": {
				"type": "string",
				"defaultValue": "microsoft"
			},
			"repo": {
				"type": "string",
				"defaultValue": "sql-server-samples"
			},
			"branch": {
				"type": "string",
				"defaultValue": "master"
			},
			"path": {
				"type": "string",
				"defaultValue": "samples/databases/adventure-works/oltp-install-script"
			},
			"fileName": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "HttpServerLocation",
				"relativeUrl": {
					"value": "@concat(\n    '/', \n    pipeline().parameters.owner, '/', \n    pipeline().parameters.repo, '/', \n    pipeline().parameters.branch, '/', \n    dataset().path\n)",
					"type": "Expression"
				}
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": false,
			"quoteChar": "\""
		},
		"schema": []
	}
}